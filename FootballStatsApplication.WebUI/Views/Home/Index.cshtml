@model List<FootballStatsApplication.WebUI.Models.LeagueViewModel>
@{
    Layout = null;

}
<!doctype html>
<html>
<head>
    <title>Hello ASP.NET MVC Core</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
</head>
<body>
    <h2>FootballstatsApplication</h2>
    @for (int i = 0; i < Model.Count; i++)
    {
        <p>@Model[i].Id</p>
        <p>@Model[i].LeagueName</p>
        <p>@Model[i].CreatedOn</p>
        <p>@Model[i].Keyword</p>
        <br />
    }
</body>
</html>
