@model List<FootballStatsApplication.WebUI.Models.PlayerViewModel>
@{
    Layout = null;

}
<!doctype html>
<html>
<head>
    <title>Hello ASP.NET MVC Core</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
</head>
<body>
    <h2>FootballstatsApplication</h2>
    @for (int i = 0; i < Model.Count; i++)
    {
        <p>@Model[i].Id</p>
        <p>@Model[i].SecondName</p>
        <p>@Model[i].FirstName</p>
        <p>@Model[i].BirthDate</p>
        <p>@Model[i].FootStyle</p>
        <p>@Model[i].Avatar</p>
        <br />
    }
</body>
</html>